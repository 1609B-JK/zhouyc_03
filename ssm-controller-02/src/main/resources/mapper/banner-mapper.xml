<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >  
<mapper namespace="com.jk.mapper.banner.BannerMapper">

    <select id="selectBannerList" resultType="bannerResponse">
      SELECT
          t_id as "bannerID",
          t_title as "bannerTitle",
          t_imgpath as "bannerImgPath",
          t_linkurl as "bannerLinkUrl"
      FROM t_banners
    </select>

</mapper>   